n    }\n\n    deleteIt = (e) => {\n        e.stopPropagation()\n        this.props.deletePalette(this.props.id)\n    }\n\n    render() {\n        const { classes, paletteName, emoji, colors, handleClick } = this.props;\n        const miniColorBoxes = colors.map(color => (\n            <div\n                className={classes.miniColor}\n                style={{ backgroundColor: color.color }}\n                key={color.name}\n            />\n        ))\n\n        return (\n            <div className={classes.root} onClick={handleClick}>\n                <DeleteIcon\n                    className={classes.deleteIcon}\n                    onClick={this.deleteIt}\n                />\n                <div className={classes.colors}>\n                    {miniColorBoxes}\n                </div>\n                <h5 className={classes.title}>{paletteName}\n                    <span className={classes.emoji}>{emoji}</span>\n                </h5>\n\n            </div>\n        )\n    }\n}\n\n\n\nexport default withStyles(styles)(MiniPalette)","/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/SingleColorPalette.js",[],"/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/Footer.js",[],"/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/styles/ColorBoxStyles.js",[],"/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/styles/PaletteStyles.js",[],"/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/styles/MiniPaletteStyles.js",[],"/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/styles/FooterStyles.js",["129"],"export default {\n    PaletteFooter: {\n        backgroundColor: \"#fff\",\n        height: \"5vh\",\n        display: \"flex\",\n        justifyContent: \"flex-end\",\n        alignItems: \"center\",\n        fontWeight: \"bold\",\n    },\n\n    emoji: {\n        fontSize: \"1.5rem\",\n        margin: \"1rem\",\n    }\n}","/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/styles/PaletteListStyles.js