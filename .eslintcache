alette = (emoji) => {\n        const newPalette = { paletteName: this.state.newPaletteName, emoji: emoji.native }\n        this.props.handleSubmit(newPalette)\n    }\n\n    render() {\n\n        const { newPaletteName } = this.state\n        const { handleSubmit, hideForm } = this.props\n\n        return (\n            <div>\n                <Dialog open={this.state.stage === 'emoji'} onClose={hideForm} aria-labelledby=\"emoji-dialog-title\">\n                    <DialogTitle id=\"emoji-dialog-title\">Choose A Palette Emoji</DialogTitle>\n                    <Picker\n                        onSelect={this.savePalette}\n                    />\n                </Dialog>\n                <Dialog open={this.state.stage === 'form'} onClose={hideForm} aria-labelledby=\"form-dialog-title\">\n                    <DialogTitle id=\"form-dialog-title\">Choose A Palette Name</DialogTitle>\n                    <ValidatorForm onSubmit={this.showEmojiPicker}>\n                        <DialogContent>\n                            <DialogContentText>\n                                Please enter a name for your new beautiful palette.\n                                Make sure it's unique!\n                            </DialogContentText>\n\n\n                            <TextValidator\n                                label=\"Palette Name\"\n                                value={newPaletteName}\n                                name=\"newPaletteName\"\n                                fullWidth\n                                margin=\"normal\"\n                                onChange={this.handleChange}\n                                validators={[\"required\", \"isPaletteNameUnique\"]}\n                                errorMessages={['Enter a palette name', 'Name already used']}\n                            />\n\n\n                        </DialogContent>\n                        <DialogActions>\n                            <Button onClick={hideForm} color=\"primary\">\n                                Cancel\n                  </Button>\n                            <Button\n                                variant='contained'\n                                color='primary'\n                                type='submit'>\n                                Save Palette\n                            </Button>\n                        </DialogActions>\n                    </ValidatorForm>\n                </Dialog>\n            </div>\n        );\n    }\n}\n\n\nexport default PaletteDialog","/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/styles/NewPaletteFormStyles.js",[],"/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/styles/PaletteFormNavStyles.js",[],"/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/styles/ColorPickerFormStyles.js",[],"/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/styles/DraggableColorBoxStyles.js",[],"/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/constants.js",[],"/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/styles/sizes.js",[],{"ruleId":"131","replacedBy":"132"},{"ruleId":"133","replacedBy":"134"},{"ruleId":"135","severity":1,"message":"136","line":9,"column":5,"nodeType":"137","messageId":"138","endLine":12,"endColumn":6},{"ruleId":"139","severity":1,"message":"140","line":1,"column":1,"nodeType":"141","endLine":15,"endColumn":2},{"ruleId":"142","severity":1,"message":"143","line":55,"column":17,"nodeType":"144","messageId":"145","endLine":55,"endColumn":29},"no-native-reassign",["146"],"no-negated-in-lhs",["147"],"no-useless-constructor","Useless constructor.","MethodDefinition","noUselessConstructor","import/no-anonymous-default-export","Assign object to a variable before exporting as module default","ExportDefaultDeclaration","no-unused-vars","'handleSubmit' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]