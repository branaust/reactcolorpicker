nts/Projects/reactcolorpicker/src/ColorBox.js",[],"/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/colorHelpers.js",[],"/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/Navbar.js",[],"/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/PaletteList.js",[],"/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/MiniPalette.js",["137"],"import React, { Component } from 'react'\nimport { withStyles } from '@material-ui/styles'\nimport styles from './styles/MiniPaletteStyles'\nimport DeleteIcon from '@material-ui/icons/Delete'\n\n\n\nclass MiniPalette extends Component {\n    constructor(props) {\n        super(props)\n\n    }\n\n    deleteIt = (e) => {\n        e.stopPropagation()\n        this.props.openDialog(this.props.id)\n    }\n\n    render() {\n        const { classes, paletteName, emoji, colors, handleClick } = this.props;\n        const miniColorBoxes = colors.map(color => (\n            <div\n                className={classes.miniColor}\n                style={{ backgroundColor: color.color }}\n                key={color.name}\n            />\n        ))\n\n        return (\n            <div className={classes.root} onClick={handleClick}>\n                <DeleteIcon\n                    className={classes.deleteIcon}\n                    onClick={this.deleteIt}\n                />\n                <div className={classes.colors}>\n                    {miniColorBoxes}\n                </div>\n                <h5 className={classes.title}>{paletteName}\n                    <span className={classes.emoji}>{emoji}</span>\n                </h5>\n\n            </div>\n        )\n    }\n}\n\n\n\nexport default withStyles(styles)(MiniPalette)","/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/SingleColorPalette.js",[],"/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/Footer.js",[],"/Users/brandonaustin/Documents/Projects/reactcolorpicker/src/styles/ColorBoxStyles.js",[],"/Users/brandonaustin/Documents/P